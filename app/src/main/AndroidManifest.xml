<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wfrete.wfrete2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.wfrete.wfrete2.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wfrete.wfrete2.activity.MotoristaCadastrarActivity"
            android:label="@string/nav_bar_motoristas" >
        </activity>
        <activity
            android:name="com.wfrete.wfrete2.activity.VeiculoCadastrarActivity"
            android:label="@string/nav_bar_veiculos" >
        </activity>
        <activity
            android:name="com.wfrete.wfrete2.activity.FreteCadastrarActivity"
            android:label="@string/nav_bar_fretes" >
        </activity>
        <activity
            android:name="com.wfrete.wfrete2.activity.LctoCadastrarActivity"
            android:label="Lcto do Frete" >
        </activity>
        <activity
            android:name="com.wfrete.wfrete2.activity.CategoriaListarActivity"
            android:label="Lista de Categorias" >
        </activity>
    </application>

</manifest>